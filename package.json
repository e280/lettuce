{
	"name": "@e280/lettuce",
	"version": "0.1.0-0",
	"description": "splitty panelly leafy layouts",
	"license": "MIT",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"type": "module",
	"main": "./x/index.js",
	"exports": {
		".": "./x/index.js"
	},
	"files": [
		"x",
		"s"
	],
	"scripts": {
		"build": "run-s _clean _ln _tsc _scute",
		"start": "octo 'scute -vw' 'tsc -w' 'node --watch x/tests.test.js' 'http-server x'",
		"count": "find s -path '*/_archive' -prune -o -name '*.ts' -exec wc -l {} +",
		"test": "node x/tests.test.js",
		"test-inspect": "node inspect x/tests.test.js",
		"_clean": "rm -rf x && mkdir x",
		"_tsc": "tsc",
		"_scute": "scute -v",
		"_ln": "run-s _ln:s _ln:assets",
		"_ln:s": "ln -s \"$(realpath s)\" x/s",
		"_ln:assets": "ln -s \"$(realpath assets)\" x/assets"
	},
	"dependencies": {
		"@e280/sly": "^0.2.0",
		"@e280/strata": "^0.2.2",
		"@e280/stz": "^0.2.14",
		"lit": "^3.3.1"
	},
	"devDependencies": {
		"@e280/scute": "^0.1.1",
		"http-server": "^14.1.1",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.9.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/e280/lettuce.git"
	},
	"bugs": {
		"url": "https://github.com/e280/lettuce/issues"
	},
	"homepage": "https://github.com/e280/lettuce#readme",
	"keywords": [
		"splitty",
		"panelly",
		"leafy",
		"panel",
		"layout",
		"ui"
	]
}
